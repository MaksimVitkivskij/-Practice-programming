#include <stdio.h>
#include <windows.h>

int main() {
    SetConsoleCP(1251);
    SetConsoleOutputCP(1251);

    int n, i, count = 0;

    // Ввід натурального числа n
    printf("Введіть натуральне число n: ");
    scanf("%d", &n);

    // Підрахунок рівних дільників числа n
    for (i = 1; i <= n; i++) {
        if (n % i == 0) {
            count++;
        }
    }

    // Виведення результату
    printf("Кількість рівних дільників числа %d: %d\n", n, count);
        printf("\n\n Практичне завдання №4\n Виконав студент НУБІП групи ІПЗ-22009б \n Вітківський Максим Сергійович\n\n");

    return 0;
}
