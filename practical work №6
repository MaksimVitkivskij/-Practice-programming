#include <stdio.h>
#include <windows.h>
#include <math.h>

int main() {
    SetConsoleCP(1251);
    SetConsoleOutputCP(1251);

    int x1, y1, x2, y2;
    double distance;

    // Ввід координат початку вектора (x1, y1)
    printf("Введіть координати початку вектора (x1, y1): ");
    scanf("%d %d", &x1, &y1);

    // Ввід координат кінця вектора (x2, y2)
    printf("Введіть координати кінця вектора (x2, y2): ");
    scanf("%d %d", &x2, &y2);

    // Обчислення відстані між точками за допомогою формули відстані між двома точками в просторі
    distance = sqrt(pow(x2 - x1, 2) + pow(y2 - y1, 2));

    // Виведення результату, який містить довжину вектора
    printf("Довжина вектора: %.6f\n", distance);
    printf("\n\n Практичне завдання №6\n Виконав студент НУБІП групи ІПЗ-22009б \n Вітківський Максим Сергійович\n\n");

    return 0;
}
