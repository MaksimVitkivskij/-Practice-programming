#include <stdio.h>
#include <windows.h>

int main() {
    SetConsoleCP(1251);
    SetConsoleOutputCP(1251);

    int n, i;

    // Ввід довжини послідовності n
    printf("Введіть довжину послідовності n: ");
    scanf("%d", &n);

    // Ініціалізація масиву dp для збереження значень послідовності
    int dp[10001];

    // Перші два значення послідовності
    dp[1] = 2;
    dp[2] = 3;

    // Обчислення значень послідовності для i >= 3
    for (i = 3; i <= n; i++) {
        // Формула для обчислення i-го значення послідовності, яке залежить від двох попередніх
        dp[i] = (dp[i-1] + dp[i-2]) % 12345;
    }

    // Виведення результату, який містить кількість шуканих послідовностей довжиною n
    printf("Кількість шуканих послідовностей довжиною %d: %d\n", n, dp[n]);
    printf("\n\n Практичне завдання №5\n Виконав студент НУБІП групи ІПЗ-22009б \n Вітківський Максим Сергійович\n\n");

    return 0;
}
